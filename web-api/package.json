{
  "name": "web-api",
  "description": "US Tax Court Document Service API ",
  "version": "0.1.0",
  "dependencies": {
    "aws-xray-sdk": "^2.3.3",
    "eslint-plugin-sort-requires": "^2.1.0",
    "jsonwebtoken": "^8.5.1",
    "pdf-lib": "^0.6.3",
    "s3rver": "github:ustaxcourt/s3rver#created_presigned_post_support",
    "uuid": "^3.3.2"
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "artillery": "^1.6.0-28",
    "artillery-plugin-fuzzer": "^1.0.1",
    "aws-sdk": "^2.492.0",
    "aws-sdk-mock": "^4.5.0",
    "chrome-aws-lambda": "^1.18.1",
    "dynamodb-admin": "^3.1.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.1.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-jsdoc": "^8.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.2",
    "eslint-plugin-sort-keys-fix": "^1.0.1",
    "express": "^4.17.1",
    "http-proxy": "^1.17.0",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^2.7.0",
    "jest": "^24.8.0",
    "lambda-tester": "^3.6.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "proxyquire": "^2.1.0",
    "puppeteer": "~1.18.0",
    "serverless": "^1.47.0",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-apigwy-binary": "^1.0.0",
    "serverless-domain-manager": "github:codyseibert/serverless-domain-manager",
    "serverless-latest-layer-version": "^1.0.3",
    "serverless-offline": "^4.10.6",
    "serverless-plugin-aws-alerts": "^1.2.4",
    "serverless-plugin-bind-deployment-id": "github:ustaxcourt/serverless-plugin-bind-deployment-id#update_lodash",
    "serverless-plugin-git-variables": "^3.3.2",
    "serverless-plugin-split-stacks": "^1.8.0",
    "serverless-plugin-tracing": "^2.0.0",
    "serverless-prune-plugin": "^1.3.2",
    "serverless-s3-local": "github:ustaxcourt/serverless-s3-local",
    "spawn-sync": "^2.0.0",
    "swagger-cli": "^2.3.0",
    "try-thread-sleep": "^2.0.0"
  },
  "scripts": {
    "build": "parcel build --no-source-maps --bundle-node-modules --target node src/handlers.js",
    "postinstall": "npm run install:dynamodb",
    "install:dynamodb": "sls dynamodb install -s local -r us-east-1 --domain noop --accountId noop --userPoolId noop  --efcmsTableName=efcms-local | grep -v -e '^$'",
    "seed:s3": "rm -rf storage/s3/noop-documents-local-us-east-1 && cp -R storage/fixtures/s3/ storage/s3/noop-documents-local-us-east-1",
    "seed:db": "AWS_ACCESS_KEY_ID=noop AWS_SECRET_ACCESS_KEY=noop SLS_DEPLOYMENT_BUCKET=noop sls dynamodb seed --seed test --noTimeout --stage local --region us-east-1 --domain noop  --userPoolId noop --efcmsTableName=efcms-local --accountId noop",
    "install:lambda": "docker pull lambci/lambda",
    "start:dynamodb": "sls dynamodb start -p 8000 --migrate true --stage local",
    "start:local": "./run-local.sh",
    "dynamo:export": "AWS_ACCESS_KEY_ID=noop AWS_SECRET_ACCESS_KEY=noop SLS_DEPLOYMENT_BUCKET=noop node dynamo-export.js > ./storage/fixtures/efcms.json",
    "start": "./run-local.sh",
    "watch": "nodemon -e yml --exec sls offline start",
    "lint": "eslint ./src && swagger-cli validate swagger.json",
    "lint:fix": "eslint ./src --fix",
    "truffleHog": "if [ $(trufflehog --regex --entropy=True https://github.com/flexion/ustc-case-mgmt.git | grep 'Filepath:' | grep -v 'package-lock.json' | wc -l) -ne '0' ]; then echo 'WARNING! A secret key was found in the git repo!'; exit 1; fi",
    "test": "jest .*test\\.js$",
    "test:snyk": "snyk test",
    "test:watch": "jest --watch .*test\\.js$",
    "dynamo:admin": "DYNAMO_ENDPOINT=http://localhost:8000 dynamodb-admin",
    "create-tables": "AWS_ACCESS_KEY_ID=noop AWS_SECRET_ACCESS_KEY=noop node create-dynamo-tables.js",
    "docker:shellcheck": "./docker-shellcheck.sh",
    "docker:audit": "./docker-audit.sh",
    "docker:lint": "./docker-lint.sh",
    "docker:test": "./docker-test.sh",
    "docker:sonarqube": "./docker-sonarqube.sh",
    "docker:terraform": "./docker-terraform.sh",
    "docker:serverless": "./docker-serverless.sh",
    "docker:s3replication": "./docker-s3replication.sh",
    "docker:smoketests": "./docker-smoketests.sh",
    "docker:route53": "./docker-route53.sh",
    "docker:globaltables": "./docker-globaltables.sh",
    "docker:clamav": "./docker-clamav.sh",
    "docker:ghostscript": "./docker-ghostscript.sh"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  }
}
