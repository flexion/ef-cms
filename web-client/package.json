{
  "name": "ef-cms-web-client",
  "version": "1.0.0",
  "description": "Electronic Filing / Case Management System Web Client ",
  "main": "index.js",
  "scripts": {
    "cypress": "cypress run",
    "cypress:open": "CYPRESS=true cypress open --config watchForFileChanges=true",
    "dev": "FILE_UPLOAD_MODAL_TIMEOUT=1 GIT_COMMIT=$(git log --pretty=format:'%h' -n 1) USTC_ENV=dev parcel --no-source-maps src/index.pug",
    "dev:cypress": "CYPRESS=true npm run dev -- --no-cache --no-hmr --no-source-maps",
    "dev:cognito": "COGNITO=true npm run dev -- --no-cache  --no-hmr --no-source-maps",
    "dev:debug": "USTC_DEBUG=true npm run dev -- --no-cache --no-hmr --no-source-maps",
    "dist": "rm -rf dist; GIT_COMMIT=$(git log --pretty=format:'%h' -n 1) USTC_ENV=prod parcel build --no-cache --no-source-maps src/index.pug; jsdoc -c jsdoc.conf.json",
    "lint": "eslint ./src && stylelint ./src/**/*.scss",
    "lint:fix": "eslint ./src --fix && stylelint ./src/**/*.scss --fix",
    "test": "FILE_UPLOAD_MODAL_TIMEOUT=1 jest --runInBand .*test\\.js$",
    "test:all": "npm run lint && npm run test && npm run test:pa11y && npm run cypress",
    "test:e2e": "jest --runInBand --config jest-e2e.config.js ./e2e/.*test\\.js$",
    "test:pa11y": "pa11y-ci --config pa11y-ci.config.json",
    "test:unit": "jest --runInBand --config jest-unit.config.js ./src/.*test\\.js$",
    "test:watch": "jest --runInBand --watch .*test\\.js$",
    "print:success": "../wait-until.sh http://localhost:1234 && echo 'Build successful. You can access the site at http://localhost:1234'",
    "start": "npm run dev",
    "docker:init": "./docker-init.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ustaxcourt/ef-cms.git"
  },
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/ustaxcourt/ef-cms/issues"
  },
  "browserslist": [
    ">2%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 11"
  ],
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "flexbox": "no-2009"
      }
    }
  },
  "homepage": "https://github.com/ustaxcourt/ef-cms#readme",
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@cerebral/react": "^4.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-regular-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "acorn": "^6.1.1",
    "ajv": "^6.10.0",
    "autoprefixer": "^9.6.0",
    "aws-sdk": "^2.473.0",
    "axios": "^0.19.0",
    "axios-mock-adapter": "^1.16.0",
    "babel-jest": "^24.8.0",
    "babel-plugin-cerebral": "^1.0.1",
    "cerebral": "^5.1.1",
    "classnames": "^2.2.6",
    "core-js": "^2.6.8",
    "cypress": "^3.3.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsdoc": "^6.0.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.4.0",
    "eslint-plugin-sort-keys-fix": "^1.0.1",
    "form-data": "^2.3.3",
    "husky": "^2.4.1",
    "i": "^0.3.6",
    "jest": "^24.8.0",
    "jsdoc": "^3.6.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "nock": "^10.0.6",
    "node-sass": "^4.12.0",
    "pa11y-ci": "^2.3.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "pug": "^2.0.3",
    "qs": "^6.7.0",
    "query-string": "5.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-focus-lock": "^1.19.1",
    "react-idle-timer": "^4.2.5",
    "react-responsive": "^6.1.2",
    "react-select": "^2.4.3",
    "react-test-renderer": "^16.8.6",
    "riot": "^3.13.2",
    "riot-route": "^3.1.4",
    "sinon": "^7.3.2",
    "stylelint": "^10.1.0",
    "stylelint-config-idiomatic-order": "^7.0.0",
    "stylelint-config-standard": "^18.3.0",
    "uswds": "^2.0.2",
    "wicg-inert": "^2.1.0"
  }
}
