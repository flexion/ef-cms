{
  "name": "ef-cms-web-client",
  "version": "1.0.0",
  "description": "Electronic Filing / Case Management System Web Client",
  "main": "index.js",
  "scripts": {
    "cypress": "cypress run",
    "cypress:open": "cypress open --config watchForFileChanges=true",
    "dev": "USTC_ENV=dev parcel --no-cache --no-hmr src/index.pug",
    "dev:cognito": "COGNITO=true USTC_ENV=dev parcel --no-cache --no-hmr src/index.pug",
    "dev:debug": "USTC_DEBUG=true npm run dev",
    "dist": "rm -rf dist; USTC_ENV=prod parcel build --no-cache src/index.pug",
    "lint": "eslint ./src && stylelint ./src/**/*.scss",
    "test": "jest .*test\\.js$",
    "test:watch": "jest --watch .*test\\.js$",
    "test:unit": "jest --config jest-unit.config.js ./src/.*test\\.js$",
    "test:e2e": "jest --config jest-e2e.config.js ./e2e/.*test\\.js$",
    "test:pa11y": "pa11y-ci --config pa11y-ci.config.json",
    "test:all": "npm run lint && npm run test && npm run test:pa11y && npm run cypress",
    "print:success": "../wait-until.sh http://localhost:1234 && echo 'Build successful. You can access the site at http://localhost:1234/log-in'",
    "start": "npm run dev",
    "docker:init": "./docker-init.sh",
    "nightwatch": "nightwatch -e chrome,firefox,safari,ie,edge"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ustaxcourt/ef-cms.git"
  },
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/ustaxcourt/ef-cms/issues"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">2%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 11"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "homepage": "https://github.com/ustaxcourt/ef-cms#readme",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@cerebral/react": "^4.1.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-regular-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "acorn": "^6.1.1",
    "ajv": "^6.10.0",
    "autoprefixer": "^9.5.0",
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.16.0",
    "babel-jest": "^24.5.0",
    "babel-plugin-cerebral": "^1.0.1",
    "cerebral": "^5.1.0",
    "classnames": "^2.2.6",
    "cypress": "^3.2.0",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsdoc": "^4.4.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-security": "^1.4.0",
    "form-data": "^2.3.3",
    "husky": "^1.3.1",
    "i": "^0.3.6",
    "jest": "^24.5.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "nightwatch": "^1.0.19",
    "nock": "^10.0.6",
    "pa11y-ci": "^2.1.1",
    "parcel-bundler": "^1.12.2",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "pug": "^2.0.3",
    "query-string": "5.1.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-test-renderer": "^16.8.4",
    "riot": "^3.13.2",
    "riot-route": "^3.1.4",
    "sass": "^1.17.3",
    "sinon": "^7.2.7",
    "stylelint": "^9.10.1",
    "stylelint-config-idiomatic-order": "^6.2.0",
    "stylelint-config-standard": "^18.2.0",
    "uswds": "^1.6.9",
    "webdriver-manager": "^12.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "eslint-plugin-sort-keys-fix": "^1.0.1"
  }
}
