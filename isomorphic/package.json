{
  "name": "ef-cms-web-client",
  "version": "1.0.0",
  "description": "Electronic Filing / Case Management System Isomorphic Components",
  "scripts": {
    "lint": "eslint ./src; stylelint ./src/**/*.scss;",
    "test": "jest ./src/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ustaxcourt/ef-cms.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ustaxcourt/ef-cms/issues"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">2%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 11"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "homepage": "https://github.com/ustaxcourt/ef-cms#readme",
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-cerebral": "^1.0.1",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-cypress": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^23.6.0",
    "joi": "^14.0.6",
    "prettier": "^1.15.2",
    "regenerator-runtime": "^0.12.1",
    "stylelint": "^9.8.0",
    "stylelint-config-idiomatic-order": "^5.0.0",
    "stylelint-config-standard": "^18.2.0"
  }
}
