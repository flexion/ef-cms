  "scripts": {
    "build:all": "rm -rf dist; rm -rf .cache; npm run build:api && npm run build:api:users && npm run build:api:cases && npm run build:api:documents && npm run build:api:workItems  && npm run build:api:sections && npm run build:api:trialSessions && npm run build:client",
    "build:api:cases": "parcel build --no-source-maps --bundle-node-modules --target node web-api/src/casesHandlers.js",
    "build:api:documents": "parcel build --no-source-maps --bundle-node-modules --target node web-api/src/documentsHandlers.js",
    "build:api:sections": "parcel build --no-source-maps --bundle-node-modules --target node web-api/src/sectionsHandlers.js",
    "build:api:trialSessions": "parcel build --no-source-maps --bundle-node-modules --target node web-api/src/trialSessionsHandlers.js",
    "build:api:users": "parcel build --no-source-maps --bundle-node-modules --target node web-api/src/usersHandlers.js",
    "build:api:workItems": "parcel build --no-source-maps --bundle-node-modules --target node web-api/src/workItemsHandlers.js",
    "build:api": "parcel build --no-source-maps --bundle-node-modules --target node web-api/src/apiHandlers.js",
    "build:client": "USTC_ENV=prod GIT_COMMIT=$(git log --pretty=format:'%h' -n 1 2>/dev/null) parcel build --no-source-maps web-client/src/index.pug; echo jsdoc -c web-client/jsdoc.conf.json",
    "create-tables": "AWS_ACCESS_KEY_ID=noop AWS_SECRET_ACCESS_KEY=noop node web-api/create-dynamo-tables.js",
    "cypress:open": "CYPRESS=true cypress open --config watchForFileChanges=true",
    "cypress": "cypress run",
    "docker:clamav": "./web-api/docker-clamav.sh",
    "docker:ghostscript": "./web-api/docker-ghostscript.sh",
    "docker:globaltables": "./web-api/docker-globaltables.sh",
    "docker:lint": "./web-api/docker-lint.sh",
    "docker:route53": "./web-api/docker-route53.sh",
    "docker:s3replication": "./web-api/docker-s3replication.sh",
    "docker:serverless": "./web-api/docker-serverless.sh",
    "docker:shellcheck": "./docker-shellcheck.sh",
    "docker:smoketests": "./web-api/docker-smoketests.sh",
    "docker:sonarqube": "./docker-sonarqube.sh",
    "docker:terraform": "./docker-terraform.sh",
    "docker:test": "./web-api/docker-test.sh",
    "dynamo:admin": "DYNAMO_ENDPOINT=http://localhost:8000 dynamodb-admin",
    "dynamo:export": "AWS_ACCESS_KEY_ID=noop AWS_SECRET_ACCESS_KEY=noop SLS_DEPLOYMENT_BUCKET=noop node dynamo-export.js > ./storage/fixtures/efcms.json",
    "install:dynamodb": "sls dynamodb install -s local -r us-east-1 --domain noop --accountId noop --userPoolId noop  --efcmsTableName=efcms-local | grep -v -e '^$'",
    "install:lambda": "docker pull lambci/lambda",
    "lint:api": "swagger-cli validate web-api/swagger.json",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:css": "stylelint web-client/src/**/*.scss",
    "lint:fix": "eslint --ext .js,.jsx shared/src/ web-api/src/ web-client/src/ --fix; stylelint web-client/src/**/*.scss --fix",
    "lint:fixnew": "run-p lint:css:fix lint:js:fix",
    "lint:js": "eslint --ext .js,.jsx shared/src/ web-api/src/ web-client/src/",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lintnew": "run-p lint:api lint:css lint:js",
    "lint": "eslint --ext .js,.jsx shared/src/ web-api/src/ web-client/src/; stylelint web-client/src/**/*.scss; swagger-cli validate web-api/swagger.json",
    "seed:db": "AWS_ACCESS_KEY_ID=noop AWS_SECRET_ACCESS_KEY=noop SLS_DEPLOYMENT_BUCKET=noop sls dynamodb seed --seed test --noTimeout --stage local --region us-east-1 --domain noop  --userPoolId noop --efcmsTableName=efcms-local --accountId noop",
    "seed:s3": "rm -rf storage/s3/noop-documents-local-us-east-1 && cp -R storage/fixtures/s3/ storage/s3/noop-documents-local-us-east-1",
    "seed": "run-p seed:db seed:s3",
    "start:api": "./web-api/run-local.sh",
    "start:client:cypress": "CYPRESS=true npm run start:client",
    "start:client:no-scanner": "NO_SCANNER=true npm run start:client",
    "start:client": "FILE_UPLOAD_MODAL_TIMEOUT=1 GIT_COMMIT=$(git log --pretty=format:'%h' -n 1 2>/dev/null) USTC_ENV=dev parcel --no-cache --no-source-maps web-client/src/index.pug",
    "start:dynamodb": "sls dynamodb start -p 8000 --migrate true --stage local",
    "test:api": "jest --config web-api/jest.config.js \"web-api/.*\\.test\\.js\"",
    "test:client": "SKIP_SANITIZE=true SKIP_VIRUS_SCAN=true AWS_ACCESS_KEY_ID=noop AWS_SECRET_ACCESS_KEY=noop FILE_UPLOAD_MODAL_TIMEOUT=1 jest --runInBand --config web-client/jest.config.js \"web-client/.*\\.test\\.js\"",
    "test:pa11y": "pa11y-ci --config web-client/pa11y-ci.config.js",
    "test:shared": "jest --config shared/jest.config.js \"shared/.*\\.test\\.js\"",
    "test:watch": "jest --watch .*test\\.js$",
    "watch": "nodemon -e yml --exec sls offline start"
  },
  "staticFiles": {
    "staticPath": "node_modules/pdfjs-dist/build",
    "watcherGlob": false
  }
}
